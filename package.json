{
  "name": "@nivalis/eslint-config",
  "version": "1.2.0-1",
  "packageManager": "bun@1.0.11",
  "description": "ESLint shareable flat config used at Nivalis Studio",
  "type": "module",
  "keywords": [
    "eslint",
    "shareable config",
    "flat config",
    "eslintconfig"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nivalis-studio/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/nivalis-studio/eslint-config/issues"
  },
  "repository": "https://github.com/nivalis-studio/eslint-config",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "eslint": "^8.53.0"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.0.2",
    "@stylistic/eslint-plugin": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "^1.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.1.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@nivalis/prettier-config": "3.1.0",
    "@types/node": "^18.18.9",
    "eslint": "^8.53.0",
    "sucrase": "^3.34.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@nivalis/prettier-config"
}
