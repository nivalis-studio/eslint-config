{
  "name": "@nivalis/eslint-config",
  "version": "1.10.2",
  "packageManager": "bun@1.0.21",
  "description": "ESLint shareable flat config used at Nivalis Studio",
  "type": "module",
  "keywords": [
    "eslint",
    "shareable config",
    "flat config",
    "eslintconfig"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nivalis-studio/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/nivalis-studio/eslint-config/issues"
  },
  "repository": "https://github.com/nivalis-studio/eslint-config",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts": "tsc --noEmit",
    "test": "exit 0",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@next/eslint-plugin-next": "^14.1.0",
    "@stylistic/eslint-plugin": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-tailwindcss": "3.14.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-yml": "^1.12.2",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.2.5",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@nivalis/prettier-config": "3.2.5",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": "@nivalis/prettier-config"
}
